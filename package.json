  "main": "index.js",
  "scripts": {
    "start": "npm run server:start",
    "server:start": "node .",
    "server:start:watch": "nodemon --exec \"npm run server:start\" .",
    "server:build": "babel src -d build",
    "server:build:watch": "rm -r build/* ; babel src -d build -w",
    "assets:build": "webpack --config config/webpack.config.js --progress --profile --colors --display-error-details --display-cached",
    "assets:build:watch": "webpack --config config/webpack.config.js --progress --profile --colors --watch --display-error-details --display-cached",
    "assets:build:prod": "NODE_ENV=production webpack --config config/webpack.config.js --progress --profile --colors --display-error-details --display-cached --bail"
  },
  "dependencies": {
    "caporal": "^0.10.0",
    "content-type": "^1.0.4",
    "cookie": "^0.3.1",
    "find-my-way": "^1.10.1",
    "knex": "^0.15.2",
    "micro": "^9.1.0",
    "mime-types": "^2.1.17",
    "multiparty": "^4.1.3",
    "mysql": "^2.14.1",
    "qs": "^6.5.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "recursive-readdir": "^2.2.1",
    "source-map-support": "^0.5.13",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "css-loader": "^1.0.0",
    "form-serialize": "^0.7.2",
    "mini-css-extract-plugin": "^0.4.3",
    "postcss-loader": "^3.0.0",
    "promise-polyfill": "^6.0.2",
    "regenerator-runtime": "^0.11.0",
    "style-loader": "^0.23.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.3.12"
  }
}
